import java.util.Scanner;

public class aulafor01 {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n;
        int maior = -1;
        for (int i = 0; i < 10; i++) {
            n = s.nextInt();
            if (n > maior)
                maior = n;

        }
        System.out.println("maior: " + maior);
    }
}
